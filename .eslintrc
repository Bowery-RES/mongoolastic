{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "trailingComma": "es5",
      "singleQuote": true,
      "printWidth": 80
    }],
    "no-underscore-dangle": 0,
    "no-plusplus": 0
  },
  "env": {
    "mocha": true
  },
  "plugins": [
    "prettier",
    "chai-expect"
  ]
}
